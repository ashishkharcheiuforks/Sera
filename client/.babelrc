{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-transform-dynamic-import",
    "babel-plugin-styled-components",
    [
      "module-resolver",
      {
        // "root": ["./src/**"],
        "alias": {
          "~": "./src"
        }
      }
    ]
    // [
    //   "babel-plugin-root-import",
    //   {
    //     "rootPathPrefix": "~",
    //     "rootPathSuffix": "./src"
    //   }
    // ]
  ],
  "comments": true
}
